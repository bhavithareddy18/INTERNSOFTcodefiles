package pack1;
class A
{
  static int i;
  public static void main(String[] arg)
  {
    System.out.println(i);
  }
}

package pack1;
class B
{
  static int i;
  static double j;
  static boolean k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(k);
  }
}

package pack1;
class C
{
  static int i;
  static int j=10;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class D
{
  static int i;
  static int j;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    i=10;
    j=20;
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class E
{
  static int i,j,k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(k);
  }
}

package pack1;
class F
{
  static int i,j=10,m=20,k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(m);
    System.out.println(k);
  }
}

