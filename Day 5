    System.out.println(i);
    System.out.println(Q.i);
  }
}

package pack1;
class R
{
  static int i=10;
  static int j=i;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class U
{
  static int i=j;
  static int j=10;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class V
{
  static int i=10;
  static int j=test();
  static int test()
  {
    return 20;
  }
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

class X
{
  static int test1()
  {
    System.out.println("from test1");
    return 1;
  }
  static int i=test1() + test2();
  static int test1()
  {
    System.out.println("from test2");
    return 2;
  }
  public static void main(String[] arg)
  {
    System.out.println(i);
  }
}

package pack1;
class Y
{
  static int i=10;
  static int j=test();
  static int test()
  {
    System.out.println("from test1");
    return i;
  }
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

