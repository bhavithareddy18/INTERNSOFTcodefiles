package pack1;
class A
{
  static int i;
  public static void main(String[] arg)
  {
    System.out.println(i);
  }
}

package pack1;
class B
{
  static int i;
  static double j;
  static boolean k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(k);
  }
}

package pack1;
class C
{
  static int i;
  static int j=10;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class D
{
  static int i;
  static int j;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    i=10;
    j=20;
    System.out.println(i);
    System.out.println(j);
  }
}

package pack1;
class E
{
  static int i,j,k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(k);
  }
}

package pack1;
class F
{
  static int i,j=10,m=20,k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(m);
    System.out.println(k);
  }
}

package pack1;
class G
{
  static int i;
  static double j,m=30.9,k;
  public static void main(String[] arg)
  {
    System.out.println(i);
    System.out.println(j);
    System.out.println(m);
    System.out.println(k);
  }
}

package pack1;
class H
{
  static int i;
  static void test()
  {
    System.out.println("from test:"+i);
  }
  public static void main(String[] arg)
  {
    System.out.println("main:"+i);
    i=10;
    test();
  }
}

package pack1;
class I
{
  static int x;
  static void test()
  {
    System.out.println("from test:"+x);
    x=10;
  }
  public static void main(String[] arg)
  {
    System.out.println("main1:"+x);
    x=20;
    test();
    System.out.println("main2:"+x);
  }
}

package pack1;
class J
{
  static int x=0;
  static void test1()
  {
    System.out.println("test1:"+x);
    x=1;
  }
  static void test2()
  {
    System.out.println("test2:"+x);
    x=2;
  }
  public static void main(String[] arg)
  {
    test1();
    System.out.println("main1:"+x);
    test2();
    System.out.println("main2:"+x);
  }
}

package pack1;
class K
{
  static int i=0;
  public static void main(String[] arg)
  {
    System.out.println("a:"+i);
    int i=10;
    System.out.println("b:"+i);
    i=20;
    System.out.println("c:"+i);
  }
}

