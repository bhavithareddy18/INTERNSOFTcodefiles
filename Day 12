package pack1;
class P
{
  public static void main(String[] arg)
  {
    System.out.println("main begin");
    if(true)
    {
      System.out.println("if-1-begin");
      if(false)
      {
        System.out.println("if 2 begin");
        if(true)
        {
          System.out.println("if 3 begin");
          System.out.println("if 3 end");
        }
        System.out.println("if 2 end");
      }
      System.out.println("if-1-end");
    }
    System.out.println("main end");
  }
}

package pack1;
class Q
{
  public static void main(String[] arg)
  {
    System.out.println("main begin");
    if(true)
    {
      System.out.println("if-1-begin");
      if(false)
        if(true)
          System.out.println("from if 3");
      System.out.println("if-1-end");
    }
    System.out.println("main end");
  }
}

package pack1;
class R
{
  public static void main(String[] arg)
  {
    System.out.println("main begin");
    if(true)
      if(false)
        if(true)
          System.out.println("from if 3");
    System.out.println("main end");
  }
}

