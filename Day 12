{
  public static void main(String[] arg)
  {
    System.out.println("main begin");
    if(true)
    if(false)
    if(true)
    System.out.println("from if 3");
    System.out.println("main end");
  }
}

package pack2;
class A
{
  public static void main(String[] arg)
  {
    System.out.println("main begin");
    if(true)
    {
      System.out.println("if block");
    }
    else
    {
      System.out.println("From else block");
    }
  }
}

