package pack1;
public class A
{
  public static void main(String[] args)
  {
    System.out.println(1);
    int i=10/0;
    System.out.println(2);
  }
}

package pack1;
public class B
{
  public static void main(String[] args)
  {
    int[] x={12,20,40};
    System.out.println(1);
    int i=x[9];
    System.out.println(2);
  }
}

package pack1;
public class A1
{
  public static void main(String[] args)
  {
    System.out.println(1);
    test();
    System.out.println(2);
  }
  static void test()
  {
    System.out.println(3);
    int i=10/0;
    System.out.println(4);
  }
}

package pack1;
public class N
{
  public static void main(String[] args)
  {
    int i=0;
    try
    {
      System.out.println(i);
    }
    catch(ArithmeticException ex)
    {
      System.out.println(i);
    }
    System.out.println(i);
  }
}

